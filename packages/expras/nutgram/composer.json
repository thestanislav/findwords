{
    "name": "expras/nutgram",
    "description": "Nutgram integration for ExprAs framework",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ],
    "require": {
        "php": "^8.1",
        "nutgram/nutgram": "^4.36",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-stdlib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ExprAs\\Nutgram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExprAs\\Nutgram\\Test\\": "test/"
        }
    },
    "extra": {
        "expras-module": true
    }
}
