APP_ENV=production
APP_DEBUG=0
TZ=Europe/Moscow

# Database (used by Doctrine/Mezzio config)
DATABASE_HOST=mezzio-mysql
DATABASE_PORT=3306
DATABASE_NAME=app_db
DATABASE_USER=app_user
DATABASE_PASSWORD=change_me_in_production

# Server (mezzio-swoole). Use 0.0.0.0 so host can reach the app via published port (not 127.0.0.1).
SERVER_HOST=0.0.0.0
SERVER_PORT=80
SWOOLE_HOST=0.0.0.0
SWOOLE_PORT=8002

# Memcached (cache + sessions)
MEMCACHED_HOST=mezzio-memcached
MEMCACHED_PORT=11211
