# Optional overrides for docker-compose (copy to .env and customize)
# Used by docker-compose.override.yml for environment substitution

CI_SWOOLE_APP_IMAGE=mezzio-swoole-app-dev
CI_ENVIRONMENT_NAME=dev

APP_ENV=development
APP_DEBUG=1

# Mezzio Swoole - override config/autoload/mezzio-swoole.*.php
SWOOLE_HOST=0.0.0.0
SWOOLE_PORT=80

# Config cache - override config aggregator
# CONFIG_CACHE_ENABLED=0

# Memcached (used by cache and sessions)
# MEMCACHED_HOST=mezzio-memcached
# MEMCACHED_PORT=11211
